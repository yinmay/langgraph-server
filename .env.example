# DeepSeek API Key (required)
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com

# Tavily API Key (required for search tool)
TAVILY_API_KEY=your_tavily_api_key_here

# PostgreSQL Database (required for chat history persistence in development)
# Format: postgresql://username:password@host:port/database
DB_URL=postgresql://username:password@localhost:5432/langgraph_db

# PostgreSQL Database (used by LangGraph Server in production deployment)
LANGGRAPH_POSTGRES_URI=postgresql://username:password@localhost:5432/langgraph_db

# Server Configuration
NODE_ENV=development

# LangSmith (optional - for tracing and monitoring)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=langgraph-server
